{
	"editor.formatOnSave": true,
	"cSpell.words": [
		"esimaccess",
		"iccid",
		"imsis",
		"msisdn",
		"qrcode",
		"travelpulse"
	],
	"tailwindCSS.experimental.classRegex": [
		[
			"cva\\(([^)]*)\\)",
			"[\"'`]([^\"'`]*).*?[\"'`]"
		],
		[
			"cx\\(([^)]*)\\)",
			"(?:'|\"|`)([^']*)(?:'|\"|`)"
		]
	],
	"css.customData": [
		".vscode/tailwind.json"
	],
	"github.copilot.editor.enableAutoCompletions": true
}
